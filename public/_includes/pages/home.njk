---
page_id: home
---

<!DOCTYPE html>
<html lang="{{ lang }}">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="title" content="{{ meta.title }}">
    <meta name="description" content="{{ meta.description }}">
    <meta name="keywords" content="{{ meta.keywords }}">
    <title>{{ title }}</title>
    <link rel="icon" type="image/x-icon" href="/images/favicon.png">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap"
        rel="stylesheet">

    <!-- Font awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css"
        integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

    <!-- Swiper -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js" defer></script>

    <!-- AOS -->
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        window.addEventListener('DOMContentLoaded', () => {
            AOS.init()
        })
    </script>

    <!-- CSS -->
    <link rel="stylesheet" href="/shared.css">
    <link rel="stylesheet" href="/index.css">

    <!-- JS -->
    <script src="/shared.js" defer></script>
</head>

<body>
    {% include "components/nav.njk" %}

    <main>
        <section class="hero">
            <div class="hero-balls">
                <div class="hero-ball-1-container">
                    <img class="hero-ball-1" src="/images/hero-disco-ball-1.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-2-container">
                    <img class="hero-ball-2" src="/images/hero-disco-ball-2.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-3-container">
                    <img class="hero-ball-3" src="/images/hero-disco-ball-3.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-4-container">
                    <img class="hero-ball-4" src="/images/hero-disco-ball-4.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-5-container">
                    <img class="hero-ball-5" src="/images/hero-disco-ball-5.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-6-container">
                    <img class="hero-ball-6" src="/images/hero-disco-ball-6.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-7-container">
                    <img class="hero-ball-7" src="/images/hero-disco-ball-7.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-8-container">
                    <img class="hero-ball-8" src="/images/hero-disco-ball-8.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-9-container">
                    <img class="hero-ball-9" src="/images/hero-disco-ball-9.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
                <div class="hero-ball-10-container">
                    <img class="hero-ball-10" src="/images/hero-disco-ball-10.png"
                        alt="Bola de discoteca brillante para fiestas">
                </div>
            </div>
            <div class="container hero-info">
                <h1 class="h1 hero-heading" data-aos="fade-up" data-aos-offset="0">
                    {{ hero.heading }}
                </h1>
                <p class="subheading hero-subheading" data-aos="fade-up" data-aos-delay="300" data-aos-offset="0">
                    {{ hero.subheading }}
                </p>
                <a href="https://api.whatsapp.com/send?phone=19543268989" class="btn-primary" data-aos="fade-up"
                    data-aos-delay="600" data-aos-offset="0">
                    {{ hero.cta }}
                </a>
            </div>
        </section>

        <section class="section experience-section">
            <div class="container experience-info-container">
                <div class="experience-info" data-aos="zoom-in">
                    <p class="h2">+726</p>
                    <p class="subheading experience-info-subheading">
                        {{ experience.clients }}
                    </p>
                </div>

                <div class="experience-info" data-aos="zoom-in" data-aos-delay="300">
                    <p class="h2">19</p>
                    <p class="subheading experience-info-subheading">
                        {{ experience.years }}
                    </p>
                </div>

                <div class="experience-info" data-aos="zoom-in" data-aos-delay="600">
                    <p class="h2">+1000</p>
                    <p class="subheading experience-info-subheading">
                        {{ experience.events }}
                    </p>
                </div>

            </div>
        </section>

        <section id="services" class="container section services-section">
            <div class="services-container">
                <p class="subheading section-subheading-title">
                    {{ services.subheading }}
                </p>
                <h2 class="h2">{{ services.heading }}</h2>
                <div class="services">
                    <div>
                        <a href="{{ "page.wedding_planner.link" | i18n }}">
                            <div class="img-services">
                                <img class="border-radius-shadow-img" src="/images/wedding-planner-img.png"
                                    alt="{{ services.wedding_planner.img }}">
                            </div>
                            <h3 class="service-title h4">
                                {{ services.wedding_planner.heading }}
                            </h3>
                        </a>
                        <p class="subheading service-subheading-description">
                            {{ services.wedding_planner.description }}
                        </p>
                        <a href="{{ "page.wedding_planner.link" | i18n }}" class="service-link">
                            {{ i18n.see_more }}
                            <span> &rarr;</span>
                        </a>
                    </div>

                    <div>
                        <a href="{{ "page.corporative_events.link" | i18n }}">
                            <div class="img-services">
                                <img class="border-radius-shadow-img" src="/images/corporate-events-img.png" alt="{{ services.corporative_events.img }}">
                            </div>
                            <h3 class="service-title h4">
                                {{ services.corporative_events.heading }}
                            </h3>
                        </a>
                        <p class="subheading service-subheading-description">
                            {{ services.corporative_events.description }}
                        </p>
                        <a href="{{ "page.corporative_events.link" | i18n }}" class="service-link">
                            {{ i18n.see_more }}
                            <span> &rarr;</span>
                        </a>
                    </div>

                    <div>
                        <a href="{{ "page.special_occasions.link" | i18n }}">
                            <div class="img-services">
                                <img class="border-radius-shadow-img" src="/images/special-ocassion-img.png" alt="{{ services.special_occasions.img }}">
                            </div>
                            <h3 class="service-title h4">
                                {{ services.special_occasions.heading }}
                            </h3>
                        </a>
                        <p class="subheading service-subheading-description">
                            {{ services.special_occasions.description }}
                        </p>
                        <a href="{{ "page.special_occasions.link" | i18n }}" class="service-link">
                            {{ i18n.see_more }}
                            <span> &rarr;</span>
                        </a>
                    </div>

                    <div>
                        <a href="{{ "page.product_rental.link" | i18n }}">
                            <div class="img-services">
                                <img class="border-radius-shadow-img" src="/images/rental-img.png" alt="{{ services.product_rental.img }}">
                            </div>
                            <h3 class="service-title h4">
                                {{ services.product_rental.heading }}
                            </h3>
                        </a>
                        <p class="subheading service-subheading-description">
                            {{ services.product_rental.description }}
                        </p>
                        <a href="{{ "page.product_rental.link" | i18n }}" class="service-link">
                            {{ i18n.see_catalog }}
                            <span> &rarr;</span>
                        </a>
                    </div>
                </div>
            </div>
        </section>

        <section class="section process-section-home-page">
            <div class="container process-container">
                <div class="process-img-container">
                    <img class="border-radius-shadow-img" src="/images/kiss-wedding.png"
                        alt="{{ process.img1 }}">
                    <div class="overlapping-img"><img class="border-radius-shadow-img" src="/images/ceremony-room.png"
                            alt="{{ process.img2 }}"></div>
                </div>

                <div class="process-info" data-aos="fade-left" data-aos-duration="900">
                    <p class="subheading section-subheading-title">
                        {{ process.subheading }}
                    </p>
                    <h2 class="h2">{{ process.heading }}</h2>
                    <div class="process-steps">
                        <div class="step-1">
                            <div class="step-number process-step-number">01</div>
                            <div class="process-step-description step-one">
                                {{ process.step1 }}
                            </div>
                        </div>

                        <div class="step-2">
                            <div class="step-number process-step-number">02</div>
                            <div class="process-step-description">
                                {{ process.step2 }}
                            </div>
                        </div>

                        <div class="step-3">
                            <div class="step-number process-step-number">03</div>
                            <div class="process-step-description">{{ process.step3 }}</div>
                        </div>
                    </div>
                    <p class="subheading process-subheading-description">
                        {{ process.description }}
                    </p>
                    <a href="https://api.whatsapp.com/send?phone=19543268989" class="btn-primary">
                        {{ process.cta }}
                    </a>
                </div>
            </div>
        </section>

        <section class="section testimonial-section">
            <div class="container testimonial-container">
                <div class="testimonial-info-container">
                    <h2 class="h2 testimonial-container-heading">
                        {{ testimonials.heading }}
                    </h2>
                    <div class="double-quote"></div>

                    <div class="swiper">
                        <div class="swiper-wrapper">
                            <div class="swiper-slide">
                                <div class="tesimonial-slide">
                                    <p class="subheading google-review-comment">
                                        "¡Sandra es la mejor! No podemos imaginar organizar una fiesta sin ella. Hace
                                        que todo sea sencillo, asegurándose de que la comida se mantenga caliente, las
                                        bebidas frías y que cada invitado disfrute de todas las deliciosas opciones.
                                        Sandra mantiene todo funcionando sin problemas detrás de escena, permitiendo que
                                        los anfitriones se concentren en sus invitados y asegurándose de que todos pasen
                                        un tiempo maravilloso. Y lo hace todo con calma, gracia y una sonrisa..."
                                    </p>
                                    <div class="google-review-stars"></div>
                                    <p class="subheading google-review-name">Steven Villalón</p>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div class="tesimonial-slide">
                                    <p class="subheading google-review-comment">"¡Quiero dar un enorme agradecimiento a
                                        Sandra por
                                        brindar un servicio tan
                                        increíble para mi evento! Las sillas y mesas estaban en perfectas condiciones y
                                        eran exactamente lo que necesitábamos. La entrega fue puntual, y la instalación
                                        fue impecable. Su equipo fue más allá para asegurarse de que todo estuviera
                                        perfecto, y realmente lo valoro. ¡Los recomiendo mucho a cualquiera que necesite
                                        alquileres para eventos; un servicio verdaderamente excelente!"
                                    </p>
                                    <div class="google-review-stars"></div>
                                    <p class="subheading google-review-name">Yishar Rodríguez</p>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div class="tesimonial-slide">
                                    <p class="subheading google-review-comment">"Sandra me brindó un servicio increíble.
                                        Me
                                        atendió
                                        con un pedido de
                                        último minuto en Nochebuena,
                                        proporcionando mesas plegables nuevas, sillas y manteles para mi fiesta esa
                                        noche.
                                        Definitivamente seré un cliente recurrente. Recomiendo mucho sus servicios."
                                    </p>
                                    <div class="google-review-stars"></div>
                                    <p class="subheading google-review-name">Maggie Roa</p>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div class="tesimonial-slide">
                                    <p class="subheading google-review-comment">"¡Sandra y su esposo son absolutamente
                                        los mejores!
                                        Realmente marcaron la diferencia atendiendo a nuestros invitados en la fiesta de
                                        graduación de nuestro hijo. Siempre dispuestos a ayudarnos con cualquier
                                        necesidad y sirviendo con una sonrisa y de manera educada a nuestros invitados.
                                        Nos permitieron disfrutar de la fiesta con su ayuda al servir las bebidas, la
                                        comida y el pastel. ¡Muchas gracias!"
                                    </p>
                                    <div class="google-review-stars"></div>
                                    <p class="subheading google-review-name">Lorena Rincón</p>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div class="tesimonial-slide">
                                    <p class="subheading google-review-comment">
                                        "Tuve el placer de trabajar con esta increíble pareja en mi evento reciente, y
                                        no puedo elogiarles lo suficiente. Sandra y Alfredo son simplemente
                                        excepcionales. Su eficiencia, profesionalismo y conocimiento fueron realmente
                                        impresionantes. He tenido el privilegio de conocer a Sandra durante muchos años,
                                        y nunca deja de
                                        sorprenderme con su dedicación y experiencia. Si estás buscando un servicio de
                                        primer nivel para tu evento, los recomiendo de
                                        todo corazón. Son una gran ventaja para cualquier ocasión."
                                    </p>
                                    <div class="google-review-stars"></div>
                                    <p class="subheading google-review-name">Debbie S</p>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div class="tesimonial-slide">
                                    <p class="subheading google-review-comment">"Definitivamente recomendaría esta
                                        empresa. Son
                                        súper responsables y brindan un servicio de 5 estrellas. Los utilizo de tres a
                                        cuatro veces al año en los últimos cinco años."
                                    </p>
                                    <div class="google-review-stars"></div>
                                    <p class="subheading google-review-name">Panda Creative</p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="testimonial-controls">
                        <svg class="swiper-button-prev" viewBox="0 0 16 23" fill="none"
                            xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect x="16" y="23" width="16" height="23" transform="rotate(180 16 23)"
                                fill="url(#pattern0_62296_3420)" fill-opacity="0.8" />
                            <defs>
                                <pattern id="pattern0_62296_3420" patternContentUnits="objectBoundingBox" width="1"
                                    height="1">
                                    <use xlink:href="#image0_62296_3420"
                                        transform="matrix(0.00970874 0 0 0.00653595 -0.737864 -0.333333)" />
                                </pattern>
                                <image id="image0_62296_3420" width="256" height="256"
                                    xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                        <div class="swiper-pagination"></div>
                        <svg class="swiper-button-next" viewBox="0 0 20 25" xmlns="http://www.w3.org/2000/svg">
                            <rect width="20" height="25" fill="url(#pattern0_62296_3419)" fill-opacity="0.8" />
                            <defs>
                                <pattern id="pattern0_62296_3419" patternContentUnits="objectBoundingBox" width="1"
                                    height="1">
                                    <use xlink:href="#image0_62296_3419"
                                        transform="matrix(0.00830078 0 0 0.0063244 -0.5625 -0.285714)" />
                                </pattern>
                                <image id="image0_62296_3419" width="256" height="256"
                                    xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </div>
                </div>

                <div>
                    <img class="border-radius-shadow-img" src="/images/testimonial-img.png"
                        alt="{{ testimonials.img }}">
                </div>
            </div>
        </section>

        <div class="find-us-and-gallery-bg">
            <section class="section gallery-section">
                <div class="container">
                    <div class="gallery-info-section">
                        <h2 class="h2">
                            {{ gallery.heading }}
                        </h2>
                        <a href="{{ "page.gallery.link" | i18n }}" class="btn-secondary">
                            {{ i18n.see_more }}
                        </a>
                    </div>
                    <div class="gallery-photos">
                        <div data-aos="zoom-in-up" data-aos-duration="600" data-aos-offset="0"><img
                                class="border-radius-shadow-img" src="/images/gallery-photo-1.png"
                                alt="{{ gallery.img1 }}"></div>
                        <div data-aos="zoom-in-up" data-aos-delay="300" data-aos-duration="600" data-aos-offset="0"><img
                                class="border-radius-shadow-img" src="/images/gallery-photo-2.png"
                                alt="{{ gallery.img2 }}"></div>
                        <div data-aos="zoom-in-up" data-aos-delay="600" data-aos-duration="600" data-aos-offset="0"><img
                                class="border-radius-shadow-img" src="/images/gallery-photo-3.png"
                                alt="{{ gallery.img3 }}"></div>
                    </div>
                </div>
            </section>

            <section class="find-us-section section">
                <div class="container find-us-container">
                    <iframe class="find-us-map" title="SOS Party Rental Florida | Google Maps"
                        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d255348.37250196404!2d0!3d0!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xacd68d155083dfa5%3A0x5e727525b677cc5a!2sSOS%20Party%20Rental!5e0!3m2!1sen!2suk!4v1736327185520!5m2!1sen!2suk"
                        width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                        referrerpolicy="no-referrer-when-downgrade"></iframe>
                    <div class="find-us-info">
                        <h2 class="h2 find-us-heading">
                            {{ find_us.heading }}
                        </h2>
                        <p class="subheading">
                            8131 Southgate Blvr N. Lauderdale, FL 33068
                        </p>
                        <div class="subheading">
                            <p>{{ i18n.monday }}: 7:00 - 18:00</p>
                            <p>{{ i18n.tuesday }}: 7:00 - 18:00</p>
                            <p>{{ i18n.wednesday }}: 7:00 - 18:00</p>
                            <p>{{ i18n.thursday }}: 7:00 - 18:00</p>
                            <p>{{ i18n.friday }}: 7:00 - 18:00</p>
                            <p>{{ i18n.saturday }}: 7:00 - 18:00</p>
                            <p>{{ i18n.sunday }}: 8:00 - 12:00</p>
                        </div>
                    </div>

                </div>
            </section>
        </div>

        <section class="section faq-section ">
            <div class="container">
                <h2 class="h2 faqs-heading">{{ faq.heading }}</h2>

                <div class="faqs-list">
                    {% for item in faq.items %}
                    <div class="faq">
                        <button class="faq-header">
                            <span class="subheading">
                                {{ item.question }}
                            </span>
                            <svg class="faq-header-chevron" viewBox="0 0 12 8" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path
                                    d="M9.87498 1.00001L5.99498 4.88001L2.11498 1.00001C1.92814 0.812752 1.67449 0.70752 1.40998 0.70752C1.14546 0.70752 0.891807 0.812752 0.704976 1.00001C0.314976 1.39001 0.314976 2.02001 0.704976 2.41001L5.29498 7.00001C5.68498 7.39001 6.31498 7.39001 6.70498 7.00001L11.295 2.41001C11.685 2.02001 11.685 1.39001 11.295 1.00001C10.905 0.620005 10.265 0.610005 9.87498 1.00001Z"
                                    fill="#666F77" fill-opacity="1" />
                            </svg>
                        </button>
                        <div class="faq-body">
                            <p class="subheading">
                                {{ item.answer }}
                            </p>
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </section>
    </main>

    {% include "components/footer.njk" %}
    {% include "components/whatsapp-button.njk" %}
</body>

</html>
